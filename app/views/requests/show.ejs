<div class="page-header">
  <h1><%- r.req.method + ' ' + r.req.url %></h1>
</div>

<table class="table table-bordered">
  <tbody>
  <% r.req.headers && Object.keys(r.req.headers).forEach(function(key) { %>
  <tr>
    <td><%- key %></td>
    <td><%- r.req.headers[key] %></td>
  </tr>
  <% }) %>
  </tbody>
</table>


<div class="well"><%- util.inspect(r.req.any, {depth: 0}) %></div>

<div class="well"><%- r.res %></div>

