<ul class="breadcrumb">
    <li><a href="/">Home</a> <span class="divider">/</span></li>
    <li><a href="<%- pathTo.requests() %>">requests</a> <span class="divider">/</span></li>
    <li class="active"><a href="#">New request</a></li>
</ul>

<div class="page-header">
    <h1>New request</h1>
</div>

<% var form = formFor(request, {action: pathTo.requests(), method: 'POST', id: "request-form", class: 'form-horizontal'}) %>
<%- form.begin() %>
<%- include _form %>
<div class="form-actions">
    <%- form.submit('<i class="icon-ok icon-white"></i> Create request', {class: 'btn btn-primary'}) %> or
    <%- linkTo('Cancel', pathTo.requests(), {class: 'btn'}) %>
</div>
<%- form.end() %>
